{
    "name": "laravel/laravel",
    "type": "project",
    "version": "1.0.0",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^3.8",
        "godruoyi/php-snowflake": "^2.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "karyamedia/ipay88": "dev-master",
        "kotchuprik/php-short-id": "^1.1",
        "laravel/framework": "^11.0",
        "laravel/pulse": "@beta",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "*",
        "laravel/tinker": "^2.5",
        "laravolt/avatar": "^5.1",
        "maatwebsite/excel": "^3.1",
        "prologue/alerts": "^1.2",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Backpack\\CRUD\\BackpackServiceProvider",
                "Backpack\\Generators\\GeneratorsServiceProvider",
                "Backpack\\PermissionManager\\PermissionManagerServiceProvider"
            ],
            "aliases": {
                "CRUD": "Backpack\\CRUD\\app\\Library\\CrudPanel\\CrudPanelFacade",
                "Widget": "Backpack\\CRUD\\app\\Library\\Widget"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Backpack\\CRUD\\": "packages/backpack/crud/src",
            "Backpack\\Generators\\": "packages/backpack/generators/src",
            "Backpack\\PermissionManager\\": "packages/backpack/permissionmanager/src"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
